 
 
 {% extends "layout.njk" %}

 {% block head %}
  
 {% endblock %}

 {% block content %}
  <div id="wrapper">
    <img src=" {{about.avatar_url}}" alt="Imagem de {{about.name}}">
    <h1>{{about.name}}</h1>
    <h2>{{about.role}}</h2>
    <p>{{about.description}}</p>
  
  </div>
  <div id="footer" class="links">
    {% for link in about.links %}
    <a href="{{link.url}}" target="_blank">{{link.name}}</a> 
    {% endfor %}
  </div>
  <script src="script.js"></script>
 {% endblock %}